type Size: Number

model Friend(id, name) {
    id: Number,
    name: String,
}

model User(id, name) {
    id: Number,
    name: String,
    admin: Boolean,
    friends: Friend*,
}

model Yellow(id) {
    id: Number,
    size: Size?,
    meep: model Meep(id) {
        id: Number,
    }+,
    parents: User+,
    tags: String*,
}